<script>
    function FillCompalaints() {
        var Month_No = $('#ComplSummMonth_Id').val();
        var YearNo = 2022;// $('#ComplSummMonth_Id').val();
        //alert('after month capture');

        $.ajax({
            url: "@Url.Action("ComplaintsRecords", "Account")",
            type: "GET",
            dataType: "JSON",
            data: { Mobile: '0', month: Month_No, year: YearNo },
            success: function (Data) {
                //alert('after data fetch');
                var Counter = Data.length;
                var open = 0; var pro = 0; var res = 0; hold = 0; close = 0;

                for (var i = 0; i < Data.length; i++) {
                    var status = Data[i].StatusTypeId;
                    if (status == 1) { open++; }
                    else if (status == 2) { pro++; }
                    else if (status == 3) { res++; }
                    else if (status == 4) { close++; }
                    else if (status == 5) { hold++; }
                }
                close = Counter - (open+pro+res+hold);

                //$("#SubCategoryId").append(" <option label=Choose one></option>");
                document.getElementById("lblTtlCompl").innerHTML = Counter;
                document.getElementById("lblOpenCompl").innerHTML = open;
                document.getElementById("lblInProCompl").innerHTML = pro;
                document.getElementById("lblResolvedCompl").innerHTML = res;
                document.getElementById("lblHoldCompl").innerHTML = hold;
                document.getElementById("lblClosedCompl").innerHTML = close;
            }
        });
    }
    
    function FillEveList() {
        $("#tblData tbody tr").remove();
        $('#eventData').attr("hidden", true);
        var Month_No = $('#EventSummMonth_Id').val();
        var YearNo = 2022;// $('#ComplSummMonth_Id').val();
        //alert('after month capture');

        $.ajax({
            url: "@Url.Action("EventList", "Tasks")",
            type: "GET",
            dataType: "JSON",
            data: { month: Month_No, year: YearNo },
            success: function (Data) {
                //alert('after data fetch event');
                var Counter = Data.length;
                document.getElementById("lblTtlEvent").innerHTML = Counter;

                for (var i = 0; i < Data.length; i++) {
                    var transaction = '<tr style="border-bottom: solid; border-width: 1px; border-color: orange;" ><td style="width:15%">' + Data[i].EventNumber + '</td><td style="width:15%">' + Data[i].StartdateS + '</td><td style="width:55%"><div style="overflow:auto; word-wrap: break-word; height:aboslute; width:100%; scrollbar:none;" id="eventName">' + Data[i].EventName + '</div></td><td style="width:15%; text-align:center;">' + Data[i].EndDateS + '</td><td><input id="eventId_' + i + '" value = ' + Data[i].EventId + ' type="hidden" /> </td><td><input id="subEventId_' + i + '" value = ' + Data[i].SubEventId + ' type="hidden" /> </td></tr>';

                    $('#tblData tbody').append('<tr><td style="height:10px;"></td></tr>');
                    $('#tblData tbody').append(transaction);
                    $('#tblData tbody').append('<tr><td style="height:10px;"></td></tr>');
                    $('#eventData').attr("hidden", false);
                }
            }
        });
    }


</script>






